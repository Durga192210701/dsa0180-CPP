#include<iostream>
using namespace std;
int add(int a,int b){
	return a+b;
}
float add(float a,float b){
	return a+b;
}
int main(){
	int intadd=add(2,2);
	float floatadd=add(2.2f,8.2f);
	cout<<"the sum of the integers is:"<<intadd<<endl;
	cout<<"the floating point of the numbers is:"<<floatadd<<endl;
	return 0;
}

